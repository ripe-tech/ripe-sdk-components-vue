{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        { "src": "/storybook", "dest": "/storybook/index.html" },
        { "src": "/(?!res)(.*)", "dest": "/dist/styleguide/$1" }
    ]
}
