{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        { "src": "/docs/(*.)", "dest": "/styleguide/$1" }
        { "src": "/(!?res)(*.)", "dest": "/storybook/index.html" },
    ]
}
